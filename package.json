{
  "name": "tf-connected",
  "version": "2.0.0",
  "description": "A Utility to show connected Tinkerforge Bricks and Bricklets!",
  "main": "./bin/command.js",
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text-lcov ava ",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fscherwi/tf-connected.git"
  },
  "author": "fscherwi",
  "license": "MIT",
  "keywords": [
    "tinkerforge",
    "hardware",
    "brick",
    "bricklet"
  ],
  "bugs": {
    "url": "https://github.com/fscherwi/tf-connected/issues"
  },
  "homepage": "https://github.com/fscherwi/tf-connected/blob/master/README.md",
  "dependencies": {
    "commander": "^2.19.0",
    "replace-string": "^2.0.0",
    "table": "^5.2.1",
    "tinkerforge": "^2.1.21"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "codacy-coverage": "^3.4.0",
    "coveralls": "^3.0.2",
    "nyc": "^13.1.0",
    "xo": "^0.24.0"
  },
  "preferGlobal": true,
  "bin": {
    "tf-connected": "./bin/command.js"
  },
  "files": [
    "src/connected.js",
    "src/get-name.js",
    "bin/command.js"
  ],
  "xo": {
    "rules": {
      "unicorn/no-process-exit": false,
      "max-params": 0
    }
  }
}
